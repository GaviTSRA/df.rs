{
  "name": "dfrs-language-support",
  "displayName": "df.rs Language Support",
  "description": "",
  "version": "0.0.1",
  "categories": [
    "Programming Languages"
  ],
  "main": "./scripts/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dfrs",
        "aliases": [
          "DiamondFire.rs",
          "dfrs"
        ],
        "extensions": [
          ".df"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dfrs",
        "scopeName": "source.dfrs",
        "path": "./syntaxes/dfrs.tmLanguage.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/vscode": "^1.75.1",
    "@vscode/test-electron": "^2.3.8"
  }
}
